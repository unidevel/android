<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:wiyun="http://schemas.android.com/apk/res/com.unidevel.barwidget"  
  android:layout_width="fill_parent" 
  android:layout_height="fill_parent"
  android:orientation="vertical">
  <LinearLayout android:orientation="horizontal" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_gravity="bottom">
  	<Button android:id="@+id/btn_ok" android:text="@android:string/ok" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:textSize="16dp"></Button>
  	<Button android:id="@+id/btn_cancel" android:text="@android:string/cancel" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_weight="1" android:textSize="16dp"></Button>
  </LinearLayout>
  <com.unidevel.barwidget.AppSelectListView android:layout_width="fill_parent" android:id="@+id/listApps" android:layout_height="fill_parent"/>
</LinearLayout>
